<section>
    <div class="container"> 
        <div class="row mt-4">
            <a href="/admin/add-products" class="btn btn-success ml-auto">Add Shops</a>
        </div>
        <table class="table">
  <thead>
    <tr>
      <th scope="col">no</th>
      <th scope="col">Shops</th>
      <th scope="col">category</th>
      <th scope="col">Description</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.description}}</td>
      <td><img style="width:50px; height:50px"src='{{this.image}}' alt=""></td>
    </tr>
    {{/each}}
    
  </tbody>
</table>
    </div>
</section>